# Test environment variables
# Loaded by vitest via tests/__helpers__/env.ts

# Mark as test environment
NODE_ENV=test

# Required for crypto operations
SECRET_KEY=vitest-secret-key-for-testing-only-32chars

# Salt for PBKDF2 key derivation (consistent across test runs)
ENCRYPTION_SALT=vitest-salt-for-testing

# Suppress verbose logging during tests
LOG_LEVEL=error
